<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstjavafxproject.AgeController">
   <children>
      <Label fx:id="myLabel" layoutX="268.0" layoutY="38.0" text="Hello!">
         <font>
            <Font name="Century Gothic" size="24.0" />
         </font>
      </Label>
      <Label layoutX="238.0" layoutY="121.0" text="Enter your age">
         <font>
            <Font name="Century" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="myTextField" layoutX="227.0" layoutY="188.0" />
      <Button fx:id="BtnSubmit" layoutX="267.0" layoutY="262.0" mnemonicParsing="false" onAction="#submit" text="Submit" textFill="#2b82a8">
         <font>
            <Font name="Cambria" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
