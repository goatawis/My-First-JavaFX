<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>


<AnchorPane prefHeight="399.0" prefWidth="484.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstjavafxproject.MediaViewController">
   <children>
      <MediaView fx:id="myMediaView" fitHeight="300.0" fitWidth="500.0" />
      <Button fx:id="BtnPlay" layoutX="118.0" layoutY="333.0" mnemonicParsing="false" onAction="#playMedia" text="PLAY" />
      <Button fx:id="BtnPause" layoutX="224.0" layoutY="333.0" mnemonicParsing="false" onAction="#pauseMedia" text="PAUSE" />
      <Button fx:id="BtnReset" layoutX="321.0" layoutY="333.0" mnemonicParsing="false" onAction="#resetMedia" text="RESET" />
   </children>
</AnchorPane>
